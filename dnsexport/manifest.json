{
    "manifest_version": 3,
    "name": "cPanel Zone Records Exporter",
    "version": "1.0",
    "description": "Export DNS zone records from cPanel to BIND format for Cloudflare.",
    "permissions": ["activeTab", "scripting", "downloads"],
    "host_permissions": ["*://*/"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  